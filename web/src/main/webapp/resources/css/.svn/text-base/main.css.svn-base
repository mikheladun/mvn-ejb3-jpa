#container { width:100%; height:100%; }
#container .wrapper{width:1000px; margin-left: 220px;padding:20px;padding-top:80px;}
#content { clear:both; height:100%; }
#content #maincontent { padding:0 !important; background-color:#f5f5f5; background-image: url('../../resources/images/bg-content-r.jpg'); background-repeat:repeat-y; background-position:100% 0%; }
#maincontent .contentdiv-border.border-bottom-left { padding:0 0 0 0 !important; margin-right:.5em; text-align:left; background-image: url('../../resources/images/bg-content-b.jpg'); background-repeat:repeat-x; background-position:0% 70%; }
#maincontent .contentdiv-border.border-bottom-left img { margin-top:0; border:0; } 
#maincontent .contentdiv-border.border-top-right { height:30px; padding:0 !important; text-align:right; background-image: url('../../resources/images/bg-content-tr.jpg'); background-repeat:no-repeat; background-position:100% 0%; }
#maincontent .contentdiv-border.border-top-right img { margin-top:.2em; border:0; }
#maincontent .contentdiv-border.border-top-right h4.title { font-size:110%; font-weight:normal; margin:.7em 0 0 .5em; float:left; padding:0; padding-left:.5em; text-transform:capitalize; color:#666; display:inline-block; }
#maincontent .contentdiv-body { padding:0 1em 0 .5em; margin:0 !important; }
#maincontent .contentdiv-body th { border-right:1px solid #e3e3e3; }
#maincontent .contentdiv-body th.last { border:0; }
#maincontent .contentdiv-body h5.title { margin-left:.9em; }
#maincontent .contentdiv-body .column-group.two-column { clear:both; }

.breadcrumb { padding-bottom:20px; margin:1em 0; clear:both; }
.breadcrumb li {float:left; padding:0 .5em 0 0 ; list-style: none; display:inline-block; }
.buttons { text-align:right; padding:1em 0; margin-right:0; }
.buttons.center { padding:.7em 0; text-align:center !important; background-color:#ccc; }
.buttons input { margin-right:1em; }
.buttons .yui3-link { margin-right:2em; text-decoration:underline; }

.yui3-form-aligned .yui3-control-group label { width:140px !important; }
.yui3-form-aligned .yui3-control-group input, 
.yui3-form-aligned .yui3-control-group select, 
.yui3-form-aligned .yui3-control-group textarea { width:280px !important; }
.yui3-form-aligned .yui3-control-group input.placeholder { color: #aaa; }
.yui3-form-aligned .yui3-control-group input.error,
.yui3-form-aligned .yui3-control-group select.error,
.yui3-form-aligned .yui3-control-group textarea.error { border-color: #f00; !important; }
.yui3-form-aligned .yui3-control-group span.error { margin-left:160px !important; margin-top:.1em !important; display:inline-block !important; }
.yui3-form-aligned .yui3-control-group span.error.hidden { display:none !important; }
.yui3-form-aligned .yui3-control-group.comments label { vertical-align:top; }
.yui3-form-aligned .yui3-control-group label.hidden { width:0px !important; }
.yui3-form-aligned .yui3-control-group .data-table .image, 
.yui3-form-aligned .yui3-control-group.multiselect .image{ vertical-align:middle; pointer:cursor; width:auto !important; padding:0 !important; border:0 !important; box-shadow:none; -webkit-transition:none; -ms-transition:none; }
.yui3-form-aligned .yui3-control-group.multiselect .image { margin-left:-2.5em; margin-right:1em; } 
.yui3-form-aligned .yui3-control-group.multiselect .remove { margin:0 0 0 .8em !important; }
.yui3-form-aligned .yui3-control-group.multiselect .multiselect { margin-top:.5em !important; margin-left:158px; display:block !important; }
.yui3-form-aligned div.multiselect { padding-right: 0em; }
.yui3-form-aligned div.multiselect span.multiselect { display:inline-block !important; margin:.5em 0 0 11.3em !important }
.ie .yui3-form-aligned div.multiselect span.multiselect { margin-left:11.2em !important }
.yui3-form-aligned div.multiselect span.multiselect .remove { margin:0 0 0 1em !important; }
.yui3-form-aligned div.multiselect span.multiselect label { margin:0 !important; width:262px !important; border:1px solid #ccc; padding:.1em .6em; } 
.column-group.two-column div.column-one, 
.column-group.two-column div.column-two { width:50%; float:left; }
.data-table .image { border:0 !important; border-radius:0 !important; box-shadow:none; }
span.expand-collapse-link { float:right; margin-top:-30px; margin-right:1em; }
div.expand-collapse { }
div.expand-collapse-div { padding-top:1.5em !important; border-top:1px solid #ddd; }
div.expand-collapse-div span.expand-collapse-link { padding:0; padding-bottom:.5em; }
div.expand-collapse-div .buttons input { width:auto !important; }


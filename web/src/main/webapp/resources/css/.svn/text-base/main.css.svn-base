.cf:before, .cf:after { content: ""; display: table; }
.cf:after { clear: both; }
.cf { zoom: 1; }
li{list-style-type:none !important;}
ul{padding:0;margin:0;}
.h{visibility:hidden;}
.n{display:none;}
.r{color:red;}
.g{color:lightgreen;}
.p{cursor:pointer;}
.l{float:left;}
.ri{float:right;}

.data-table { width:100%; margin-bottom:30px; border-bottom:1px solid #ddd; behavior: url('../../resources/pie/PIE.htc'); }
.data-table td { height:15px; padding-left:30px; border-left:1px solid #E4E4E4; }
.table-top-line { background:#e4e4e4; }
.table-odd { background:#ffffff; }
.table-even { background:#f2f2f2; }
.data-table .no-table-style { background:#f2f2f2; border:0 0 0 1px; padding-left:10px; padding:0; text-align:right; }

.top{width:1280px;height:70px;background: url('../../resources/images/top.png'); position:fixed;z-index:1;}
.logo{display:inline-block;float:left;font-size:40px;color:#666;text-decoration:none;text-shadow:0px -1px 0px #333;z-index:100;width: 200px;padding-left:20px;padding-top:7px;behavior: url('../../resources/pie/PIE.htc');}
.nav-wrap{float:right;width:1060px;height:100px;/*box-shadow: 0 8px 6px -5px #CDCDCD;*/behavior: url('../../resources/pie/PIE.htc');}
.nav{float:left;margin-left:100px;margin-top:3px;font-size:12px;}
.nav li{display:inline-block;width:110px;text-align:center;}
.nav div{width:40px; height:35px;margin-left:34px;margin-bottom:7px;padding: 1px; background: #f2f2f2; border-radius:5px;border:1px solid #666;box-shadow: 0 8px 6px -5px #999;behavior: url('../../resources/pie/PIE.htc');}
.overview{width:25px;height:25px;padding-top:4px;}
.workqueue{width:30px;height:30px;padding-top:3px;padding-left:1px;}
.personnel{width:25px;height:25px;padding-left:1px;padding-top:5px;}
.projects{width:25px;height:25px;padding-top:5px;padding-left:1px;}
.research{width:25px;height:25px;padding-top:6px;}
.nav a{text-decoration:none;color:#666;}
.right-nav{float:right;background:#666;border-top-left-radius:5px;border-bottom-left-radius:5px;margin-top:25px;padding:5px 15px 5px 15px;behavior: url('../../resources/pie/PIE.htc');}
.right-nav a{font-size:12px;color:white;text-decoration:none;}
.right-nav li{display:inline;}
.left{width:180px; height: 1000px;padding:20px;position:fixed;background: url('../../resources/images/left.png'); }
.right{width:180px; height: 1000px;padding:20px;padding-left:25px;margin-left:1060px;position:fixed;background: url('../../resources/images/right.png'); }
.search-results{margin-top:140px;height: 450px;font-size:13px;background:#f2f2f2!important;box-shadow:3px 3px 3px #CDCDCD;border-radius:7px;border-bottom:1px solid #CDCDCD;behavior: url('../../resources/pie/PIE.htc');}
.search-results div.results { height:400px;overflow:auto; }
.search-results h3,
.right-rail h3,
.indices h3{ padding-left:10px;padding-top:10px; }
.solid-top{ border-top:3px solid #CDCDCD; }
.search-person{padding-left:20px;padding-bottom:10px;padding-top:10px;float:left;border-bottom:1px dotted #CDCDCD;}
.solid-bottom{border-bottom:3px solid #CDCDCD;margin-bottom:10px;}
.search-bottom{padding-left:40px;padding-bottom:10px;}

